{
    "name": "@bucknet/web3-crawler",
    "version": "0.1.0",
    "description": "A library for crawl data",
    "private": true,
    "license": "MIT",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "prepack": "pnpm build",
        "build": "tsup src/* --format esm,cjs --dts",
        "test": "jest",
        "prepublishOnly": "yarn run build",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "lint": "eslint \"**/*.ts\"",
        "fmt": "pnpm _fmt --write",
        "fmt:check": "pnpm _fmt --check",
        "cov:clean": "rm -rf coverage"
    },
    "devDependencies": {
        "@swc/core": "^1.3.68",
        "@swc/jest": "^0.2.26",
        "@types/jest": "^24.0.25",
        "jest-environment-jsdom": "^29.7.0",
        "tsup": "^7.1.0"
    },
    "dependencies": {
        "alchemy-sdk": "^3.4.1",
        "dotenv": "^16.4.5",
        "ethereumjs-util": "^7.1.5",
        "ethers": "^6.13.2",
        "tsx": "^4.16.3"
    }
}
